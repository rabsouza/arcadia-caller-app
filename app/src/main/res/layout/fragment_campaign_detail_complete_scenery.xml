<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.battista.arcadiacaller.fragment.detail.CampaignDetailCompleteSceneryFragment">

    <android.support.v4.widget.NestedScrollView
        android:id="@+id/detail_scroll_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/fab_margin_bottom"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/activity_vertical_margin"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:id="@+id/detail_card_view_campaign"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:foreground="?attr/selectableItemBackground"
                android:padding="@dimen/no_dimen"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="1dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="1dp"
                card_view:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_horizontal_margin"
                    android:layout_marginRight="@dimen/activity_horizontal_margin"
                    android:orientation="vertical"
                    android:padding="@dimen/element_padding">

                    <TextView
                        android:id="@+id/detail_card_view_campaign_title"
                        style="@style/CardTitle02Style"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/title_campaign_active"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:background="@color/gray"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/element_vertical_margin"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/element_horizontal_margin"
                        android:paddingRight="@dimen/element_horizontal_margin">

                        <ImageView
                            android:layout_width="28dp"
                            android:layout_height="28dp"
                            android:layout_marginRight="@dimen/element_horizontal_margin"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            app:srcCompat="@drawable/ic_menu_campaign"/>

                        <TextView
                            android:id="@+id/detail_card_view_campaign_alias"
                            style="@style/CardTextStyle"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:textStyle="bold"/>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/detail_card_view_scenery_guilds"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:foreground="?attr/selectableItemBackground"
                android:padding="@dimen/no_dimen"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="1dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="1dp"
                card_view:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_horizontal_margin"
                    android:layout_marginRight="@dimen/activity_horizontal_margin"
                    android:orientation="vertical"
                    android:padding="@dimen/element_padding">

                    <TextView
                        android:id="@+id/detail_card_view_scenery_guilds_title"
                        style="@style/CardTitle02Style"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/hint_detail_guilds"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/gray"/>

                    <LinearLayout
                        android:id="@+id/detail_card_view_scenery_guilds_blue"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_scenery_guilds_img_blue"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginRight="@dimen/element_horizontal_margin"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:scaleType="centerCrop"/>

                        <TextView
                            android:id="@+id/detail_card_view_scenery_guilds_login_blue"
                            style="@style/CardTextBoldStyle"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:gravity="center_vertical"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/detail_card_view_scenery_guilds_green"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_scenery_guilds_img_green"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginRight="@dimen/element_horizontal_margin"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:scaleType="centerCrop"/>

                        <TextView
                            android:id="@+id/detail_card_view_scenery_guilds_login_green"
                            style="@style/CardTextBoldStyle"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:gravity="center_vertical"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/detail_card_view_scenery_guilds_red"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_scenery_guilds_img_red"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginRight="@dimen/element_horizontal_margin"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:scaleType="centerCrop"/>

                        <TextView
                            android:id="@+id/detail_card_view_scenery_guilds_login_red"
                            style="@style/CardTextBoldStyle"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:gravity="center_vertical"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/detail_card_view_scenery_guilds_orange"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_scenery_guilds_img_orange"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginRight="@dimen/element_horizontal_margin"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:scaleType="centerCrop"/>

                        <TextView
                            android:id="@+id/detail_card_view_scenery_guilds_login_orange"
                            style="@style/CardTextBoldStyle"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginTop="@dimen/element_vertical_margin"
                            android:gravity="center_vertical"/>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/detail_card_view_complete_scenery"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:foreground="?attr/selectableItemBackground"
                android:padding="@dimen/no_dimen"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="1dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="1dp"
                card_view:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_horizontal_margin"
                    android:layout_marginRight="@dimen/activity_horizontal_margin"
                    android:orientation="vertical"
                    android:padding="@dimen/element_padding">

                    <TextView
                        android:id="@+id/detail_card_view_complete_scenery_title"
                        style="@style/CardTitle02Style"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/hint_detail_scenery_current"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:background="@color/gray"/>

                    <TextView
                        android:id="@+id/detail_card_view_scenery_name"
                        style="@style/CardTextStyle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"/>

                    <TextView
                        android:id="@+id/detail_card_view_scenery_location"
                        style="@style/CardTextStyle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/element_vertical_margin"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/element_vertical_margin"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginRight="@dimen/element_padding"
                            android:scaleType="fitXY"
                            android:src="@drawable/won_reward"/>

                        <TextView
                            android:id="@+id/detail_card_view_scenery_won_reward"
                            style="@style/CardTextStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/element_vertical_margin"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginRight="@dimen/element_padding"
                            android:scaleType="fitXY"
                            android:src="@drawable/won_title"/>

                        <TextView
                            android:id="@+id/detail_card_view_scenery_won_title"
                            style="@style/CardTextStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"/>

                    </LinearLayout>

                    <com.weiwangcn.betterspinner.library.material.MaterialBetterSpinner
                        android:id="@+id/detail_card_view_scenery_winner"
                        style="@style/InputStyle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/element_vertical_margin"
                        android:hint="@string/hint_detail_card_view_scenery_winner"
                        app:met_floatingLabel="highlight"/>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/detail_card_view_complete_scenery_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:foreground="?attr/selectableItemBackground"
                android:padding="@dimen/no_dimen"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="1dp"
                app:cardPreventCornerOverlap="false"
                app:cardUseCompatPadding="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="1dp"
                card_view:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_horizontal_margin"
                    android:layout_marginRight="@dimen/activity_horizontal_margin"
                    android:orientation="vertical"
                    android:padding="@dimen/element_padding">

                    <TextView
                        android:id="@+id/detail_card_view_complete_scenery_result_title"
                        style="@style/CardTitle02Style"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/hint_detail_scenery_current_result"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_marginBottom="@dimen/activity_vertical_margin"
                        android:background="@color/gray"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <View
                            android:layout_width="0dp"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_weight="1"/>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_complete_scenery_result_guild_img_blue"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"
                            android:scaleType="centerCrop"/>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_complete_scenery_result_guild_img_green"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"
                            android:scaleType="centerCrop"/>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_complete_scenery_result_guild_img_red"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"
                            android:scaleType="centerCrop"/>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/detail_card_view_complete_scenery_result_guild_img_orange"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"
                            android:scaleType="centerCrop"/>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/colorTitle"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/CardTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/item_text_card_scenery_height"
                            android:layout_weight="1"
                            android:text="@string/hint_detail_card_view_scenery_least_deaths"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_least_deaths_blue"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_least_deaths_green"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_least_deaths_red"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_least_deaths_orange"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/colorTitle"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/CardTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/item_text_card_scenery_height"
                            android:layout_weight="1"
                            android:text="@string/hint_detail_card_view_scenery_most_coins"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_most_coins_blue"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_most_coins_green"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_most_coins_red"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_most_coins_orange"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/detail_card_view_scenery_won_reward_group"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/colorTitle"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/CardTextStyle"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/item_text_card_scenery_height"
                                android:layout_weight="1"
                                android:text="@string/hint_detail_card_view_scenery_won_reward"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_reward_blue"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_reward_green"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_reward_red"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_reward_orange"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/detail_card_view_scenery_won_title_group"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1px"
                            android:background="@color/colorTitle"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/CardTextStyle"
                                android:layout_width="0dp"
                                android:layout_height="@dimen/item_text_card_scenery_height"
                                android:layout_weight="1"
                                android:text="@string/hint_detail_card_view_scenery_won_title"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_title_blue"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_title_green"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_title_red"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                            <CheckBox
                                android:id="@+id/detail_card_view_scenery_won_title_orange"
                                android:layout_width="@dimen/item_img_width"
                                android:layout_height="@dimen/item_img_height"
                                android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                                android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        </LinearLayout>

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="@color/colorTitle"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            style="@style/CardTextStyle"
                            android:layout_width="0dp"
                            android:layout_height="@dimen/item_text_card_scenery_height"
                            android:layout_weight="1"
                            android:text="@string/hint_detail_card_view_scenery_saved_coin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_saved_coin_blue"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_saved_coin_green"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_saved_coin_red"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                        <CheckBox
                            android:id="@+id/detail_card_view_scenery_saved_coin_orange"
                            android:layout_width="@dimen/item_img_width"
                            android:layout_height="@dimen/item_img_height"
                            android:layout_marginLeft="@dimen/img_card_horizontal_margin"
                            android:layout_marginRight="@dimen/img_card_horizontal_margin"/>

                    </LinearLayout>

                </LinearLayout>

            </android.support.v7.widget.CardView>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/item_img_height"/>

        </LinearLayout>

    </android.support.v4.widget.NestedScrollView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_next_complete_scenery_campaign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/fab_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        app:layout_anchor="@id/detail_scroll_view"
        app:layout_anchorGravity="bottom|right"
        app:srcCompat="@drawable/ic_next"/>


</android.support.design.widget.CoordinatorLayout>
